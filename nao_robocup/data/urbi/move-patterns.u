function WalkPattern1.walk (nb, time) {
  if (nb > 0)
  {
    NaoMotors.loadPos ("WalkPattern1.start", time);
    NaoMotors.loadPos ("WalkPattern1.walk10", time * 3);
    while (nb > 0) {
      if (nb > 0)
        for| (var m = 1; m <= 5; m++)
          NaoMotors.loadPos ("WalkPattern1.walk" + m, time);
      nb--;
      if (nb > 0)
        for| (var m = 6; m <= 10; m++)
          NaoMotors.loadPos ("WalkPattern1.walk" + m, time);
      nb--;
    };
    NaoMotors.loadPos ("WalkPattern1.start", time * 3);
   
  }
  else if (nb < 0) 
  {
    NaoMotors.loadPos ("WalkPattern1.start", time);
    while (nb < 0) {
      if (nb < 0)
        for| (var m = 10; m >= 6; m--)
          NaoMotors.loadPos ("WalkPattern1.walk" + m, time);
      nb++;
      if (nb < 0)
        for| (var m = 5; m >= 1; m--)
          NaoMotors.loadPos ("WalkPattern1.walk" + m, time);
      nb++;
    };
    NaoMotors.loadPos ("WalkPattern1.walk10", time * 2);
    NaoMotors.loadPos ("WalkPattern1.start", time);
  };
},



